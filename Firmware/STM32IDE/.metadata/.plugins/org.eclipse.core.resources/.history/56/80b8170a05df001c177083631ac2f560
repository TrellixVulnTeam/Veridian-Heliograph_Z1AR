#include "main.h"

extern ADC_HandleTypeDef hadc; //Get the handle from main

//Variables
uint16_t superCapmV;

//Functions
void startADC(){
	HAL_ADC_Start(&hadc1);
	HAL_ADC_PollForConversion(&hadc1, HAL_MAX_DELAY);
	raw = HAL_ADC_GetValue(&hadc1);
}
